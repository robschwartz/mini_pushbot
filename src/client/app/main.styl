body
  border 0

.nav-wrapper
  height 10vh
  background-color #51556C
  display flex
  align-items center


.logo
  width 33%
  display inline-block
  background-color #474A63
  height 100%


.logo span
  font-family Helvetica, sans-serif
  color white
  height 100%
  float right
  padding-right 20px
  display flex
  justify-content center
  align-items center
  font-size x-large
  font-weight bolder
  .version
    color #898D93
    font-family Helvetica, sans-serif
    font-size x-small

.nav-items
  width 100%
  display inline
  padding-left 2%


.nav-item
  padding 15px

.right-nav-item
  float right
  padding-right 50px

.right-nav-item a
  color #898D93
  font-family Helvetica, sans-serif

.right-nav-item a:hover
  text-decoration none
  color #EDF0F4

.nav-item a
  color #898D93
  font-family Helvetica, sans-serif


.nav-item a:hover
  text-decoration none
  color #EDF0F4


#pushbot-img
  height 100%
